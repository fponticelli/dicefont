{
  "name": "dicefont",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "exportsvg": "sketchtool export slices assets/dicefont.sketch --output=assets/icons/",
    "optimizesvg": "./node_modules/.bin/svgo -f assets/icons --multipass",
    "gensvg": "npm run exportsvg && npm run optimizesvg",
    "fontcustom": "fontcustom compile assets/icons/",
    "watch-fontcustom": "fontcustom watch assets/icons/",
    "build": "npm run gensvg && npm run fontcustom"
  },
